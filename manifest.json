{
    "manifest_version": 2,
    "name": "Time is Money",
    "description": "Puts prices in terms of hours you need to work",
    "version": "1.0",
    "icons": {
        "16" : "Time-is-money.png"
    },
    "browser_action":{
        "default_popup":"app.html"
    },
    "permissions":[
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}